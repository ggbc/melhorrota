<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Social_Data_Mashup</servlet-name>
		<servlet-class>com.brhackday.ehnohs.Social_Data_MashupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Social_Data_Mashup</servlet-name>
		<url-pattern>/social_data_mashup</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>simpleyqlcallback</servlet-name>
		<servlet-class>com.simpleyql.CallbackServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>simpleyqlcallback</servlet-name>
		<url-pattern>/simpleyqlcallback</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>realizaqueryautorizada</servlet-name>
		<servlet-class>com.brhackday.ehnohs.RealizaQueryAutorizadaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>realizaqueryautorizada</servlet-name>
		<url-pattern>/realizaqueryautorizada</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>twitter_register</servlet-name>
		<servlet-class>com.brhackday.ehnohs.TwitterRegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>twitter_register</servlet-name>
		<url-pattern>/twitter_register</url-pattern>
	</servlet-mapping>
		
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
